				}

			}, {
				"path": "stockLaunch",
				"style": {
					"navigationBarTitleText": "库存上架",
					"enablePullDownRefresh": false
				}

			}, {
				"path": "implementLaunch/implementLaunch",
				"style": {
					"navigationBarTitleText": "库存上架",
					"enablePullDownRefresh": false
				}

			}, {
				"path": "stockCheck",
				"style": {
					"navigationBarTitleText": "盘点",
					"enablePullDownRefresh": false
				}

			}, {
				"path": "implementStockCheck/implementStockCheck",
				"style": {
					"navigationBarTitleText": "盘点",
