					"enablePullDownRefresh": false
				}
			}, {
				"path": "picking/picking",
				"style": {
					"navigationBarTitleText": "逐条拣货配货",
					"enablePullDownRefresh": false,
					"app-plus": {
						"titleNView": {
							"buttons": [{
									"text": ">",
									"float": "right"
								},
								{
									"text": "<",
									"float": "right"
								}
							]
