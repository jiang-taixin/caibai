			}, {
				"path": "provisionalReceipt",
				"style": {
					"navigationBarTitleText": "回货暂收",
					"enablePullDownRefresh": false
				}

			}, {
				"path": "RFIDDemo",
				"style": {
					"navigationBarTitleText": "RFID测试",
					"enablePullDownRefresh": false
				}

			}, {
				"path": "handover",
				"style": {
					"navigationBarTitleText": "交接",
